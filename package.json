{
  "name": "three-vertex-objects-workspace",
  "description": "three-vertex-objects project workspace",
  "private": true,
  "workspaces": [
    "vertex-objects",
    "three-tiled-maps",
    "three-stages",
    "textured-sprites",
    "three-display",
    "kobolde",
    "r3f-examples"
  ],
  "scripts": {
    "vertex-objects": "cd vertex-objects && yarn",
    "three-tiled-maps": "cd three-tiled-maps && yarn",
    "three-stages": "cd three-stages && yarn",
    "textured-sprites": "cd textured-sprites && yarn",
    "three-display": "cd three-display && yarn",
    "kobolde": "cd kobolde && yarn",
    "r3f-examples": "cd r3f-examples && yarn start",
    "build:vertex-objects": "yarn vertex-objects build",
    "build:three-tiled-maps": "yarn three-tiled-maps build",
    "build:three-stages": "yarn three-stages build",
    "build:textured-sprites": "yarn textured-sprites build",
    "build:three-display": "yarn three-display build",
    "build:kobolde": "yarn kobolde build",
    "test:vertex-objects": "yarn vertex-objects test",
    "test:three-tiled-maps": "yarn three-tiled-maps test",
    "test:three-stages": "yarn three-stages test",
    "test:textured-sprites": "yarn textured-sprites test",
    "test:three-display": "yarn three-display test",
    "test:kobolde": "yarn kobolde test",
    "clean:vertex-objects": "yarn vertex-objects clean",
    "clean:three-tiled-maps": "yarn three-tiled-maps clean",
    "clean:three-stages": "yarn three-stages clean",
    "clean:textured-sprites": "yarn textured-sprites clean",
    "clean:three-display": "yarn three-display clean",
    "clean:kobolde": "yarn kobolde clean",
    "fix:vertex-objects": "yarn vertex-objects fix",
    "fix:three-tiled-maps": "yarn three-tiled-maps fix",
    "fix:three-stages": "yarn three-stages fix",
    "fix:textured-sprites": "yarn textured-sprites fix",
    "fix:three-display": "yarn three-display fix",
    "fix:kobolde": "yarn kobolde fix",
    "build": "npm-run-all --serial build:vertex-objects --parallel build:three-tiled-maps build:three-stages build:textured-sprites build:three-display --serial build:kobolde",
    "test": "run-s test:vertex-objects test:three-tiled-maps test:three-stages test:textured-sprites test:three-display test:kobolde",
    "clean": "run-p clean:vertex-objects clean:three-tiled-maps clean:three-stages clean:textured-sprites clean:three-display clean:kobolde",
    "fix": "run-p fix:vertex-objects fix:three-tiled-maps fix:three-stages fix:textured-sprites fix:three-display fix:kobolde",
    "cbt": "run-s clean build test",
    "start": "servor . examples/tiled-maps-basic-layer-tiles-renderer.html ---reload --browse"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "servor": "^4.0.2"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.0.0"
}
