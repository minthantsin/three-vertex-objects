{
  "name": "three-vertex-objects-workspace",
  "description": "three-vertex-objects project workspace",
  "private": true,
  "workspaces": [
    "three-vertex-objects",
    "three-tiled-maps",
    "r3f-vertex-objects",
    "r3f-tiled-maps",
    "r3f-examples",
    "bitmap-font-tool"
  ],
  "scripts": {
    "three-vertex-objects": "cd three-vertex-objects && yarn",
    "three-tiled-maps": "cd three-tiled-maps && yarn",
    "r3f-vertex-objects": "cd r3f-vertex-objects && yarn",
    "r3f-tiled-maps": "cd r3f-tiled-maps && yarn",
    "build:three-vertex-objects": "yarn three-vertex-objects build",
    "build:three-tiled-maps": "yarn three-tiled-maps build",
    "build:r3f-vertex-objects": "yarn r3f-vertex-objects build",
    "build:r3f-tiled-maps": "yarn r3f-tiled-maps build",
    "test:three-vertex-objects": "yarn three-vertex-objects test",
    "test:three-tiled-maps": "yarn three-tiled-maps test",
    "test:r3f-vertex-objects": "yarn r3f-vertex-objects test",
    "test:r3f-tiled-maps": "yarn r3f-tiled-maps test",
    "clean:three-vertex-objects": "yarn three-vertex-objects clean",
    "clean:three-tiled-maps": "yarn three-tiled-maps clean",
    "clean:r3f-vertex-objects": "yarn r3f-vertex-objects clean",
    "clean:r3f-tiled-maps": "yarn r3f-tiled-maps clean",
    "fix:three-vertex-objects": "yarn three-vertex-objects fix",
    "fix:three-tiled-maps": "yarn three-tiled-maps fix",
    "fix:r3f-vertex-objects": "yarn r3f-vertex-objects fix",
    "fix:r3f-tiled-maps": "yarn r3f-tiled-maps fix",
    "build": "run-s build:three-vertex-objects build:three-tiled-maps build:r3f-vertex-objects build:r3f-tiled-maps",
    "test": "run-s test:three-vertex-objects test:three-tiled-maps test:r3f-vertex-objects test:r3f-tiled-maps",
    "clean": "run-p clean:three-vertex-objects clean:three-tiled-maps clean:r3f-vertex-objects clean:r3f-tiled-maps",
    "fix": "run-p fix:three-vertex-objects fix:three-tiled-maps fix:r3f-vertex-objects fix:r3f-tiled-maps",
    "cbt": "run-s clean build test",
    "start": "servor . examples/tiled-maps-basic-layer-tiles-renderer.html ---reload --browse"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "servor": "^4.0.2"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.0.0",
  "dependencies": {
    "node-gyp": "^8.1.0"
  }
}
