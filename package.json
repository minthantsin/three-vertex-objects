{
  "name": "three-vertex-objects-workspaces",
  "description": "three-vertex-objects project workspaces",
  "private": true,
  "workspaces": [
    "three-vertex-objects",
    "three-tiled-maps",
    "r3f-vertex-objects",
    "r3f-tiled-maps",
    "r3f-examples",
    "bitmap-font-tool"
  ],
  "scripts": {
    "build": "npm run build --workspace=three-vertex-objects --workspace=three-tiled-maps --workspace=r3f-vertex-objects --workspace=r3f-tiled-maps",
    "test": "npm run test --workspace=three-vertex-objects --workspace=three-tiled-maps --workspace=r3f-vertex-objects --workspace=r3f-tiled-maps -- --passWithNoTests",
    "clean": "npm run clean --workspace=three-vertex-objects --workspace=three-tiled-maps --workspace=r3f-vertex-objects --workspace=r3f-tiled-maps",
    "start": "servor . examples/tiled-maps-basic-layer-tiles-renderer.html ---reload --browse"
  },
