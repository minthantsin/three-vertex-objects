{
  "name": "three-vertex-objects-workspace",
  "description": "three-vertex-objects project workspace",
  "private": true,
  "workspaces": [
    "three-vertex-objects",
    "three-tiled-maps",
    "three-stages",
    "three-textured-sprites",
    "three-display"
  ],
  "scripts": {
    "three-vertex-objects": "cd three-vertex-objects && yarn",
    "three-tiled-maps": "cd three-tiled-maps && yarn",
    "three-stages": "cd three-stages && yarn",
    "three-textured-sprites": "cd three-textured-sprites && yarn",
    "three-display": "cd three-display && yarn",
    "build:three-vertex-objects": "yarn three-vertex-objects build",
    "build:three-tiled-maps": "yarn three-tiled-maps build",
    "build:three-stages": "yarn three-stages build",
    "build:three-textured-sprites": "yarn three-textured-sprites build",
    "build:three-display": "yarn three-display build",
    "test:three-vertex-objects": "yarn three-vertex-objects test",
    "test:three-tiled-maps": "yarn three-tiled-maps test",
    "test:three-stages": "yarn three-stages test",
    "test:three-textured-sprites": "yarn three-textured-sprites test",
    "test:three-display": "yarn three-display test",
    "clean:three-vertex-objects": "yarn three-vertex-objects clean",
    "clean:three-tiled-maps": "yarn three-tiled-maps clean",
    "clean:three-stages": "yarn three-stages clean",
    "clean:three-textured-sprites": "yarn three-textured-sprites clean",
    "clean:three-display": "yarn three-display clean",
    "fix:three-vertex-objects": "yarn three-vertex-objects fix",
    "fix:three-tiled-maps": "yarn three-tiled-maps fix",
    "fix:three-stages": "yarn three-stages fix",
    "fix:three-textured-sprites": "yarn three-textured-sprites fix",
    "fix:three-display": "yarn three-display fix",
    "build": "npm-run-all --serial build:three-vertex-objects --parallel build:three-tiled-maps build:three-stages build:three-textured-sprites",
    "test": "run-s test:three-vertex-objects test:three-tiled-maps test:three-stages test:three-textured-sprites",
    "clean": "run-p clean:three-vertex-objects clean:three-tiled-maps clean:three-stages clean:three-textured-sprites",
    "fix": "run-p fix:three-vertex-objects fix:three-tiled-maps fix:three-stages fix:three-textured-sprites",
    "cbt": "run-s clean build test",
    "start": "servor . examples/tiled-maps-basic-layer-tiles-renderer.html ---reload --browse"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "servor": "^4.0.2"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.0.0"
}
